<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LevelDownAlert</fullName>
    <active>true</active>
    <errorConditionFormula>VALUE(TEXT(PRIORVALUE(Level__c ))) &gt; VALUE(TEXT(Level__c))</errorConditionFormula>
    <errorDisplayField>Level__c</errorDisplayField>
    <errorMessage>Level cannot be decreased</errorMessage>
</ValidationRule>
